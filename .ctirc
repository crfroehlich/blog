{
  addNewline: true,
  excludes: [
    '@types',
    'typings',
    '__test__',
    '__tests__',
    'node_modules',
    'internal',
    'test-data',
    '__generated__',
    'public',
    '.cache',
    '.yarn',
    'content',
    'static',
    'gatsby'
  ],
  fileExcludePatterns: ['*.test.ts', '*.test', '*.feature.ts'],
  fileFirst: false,
  includeCWD: true,
  quote: "'",
  targetExts: [
    'ts',
    'tsx',
  ],
  useSemicolon: true,
  useTimestamp: false,
  verbose: false,
  withoutComment: false,
}
