  "scripts": {
    "build": "gatsby build",
    "build:codegen": "graphql-codegen",
    "build:prep": "yarn sortJson && yarn docs:standardize",
    "clean": "gatsby clean && rimraf .cache .tmp public __generated__ src/**/__generated__/**",
    "debug:dev": "node --nolazy node_modules/.bin/gatsby develop --inspect-brk",
    "debug:prod": "node --nolazy node_modules/.bin/gatsby build --inspect-brk",
    "deploy": "yarn build && node build/publish.js",
    "docs:standardize": "npx ./node_modules/@luddites-me/ts-tools/dist/readme/standardizeReadme.js",
    "lint": "pretty-quick --staged && yarn lint:sort && yarn lint:ts && yarn lint:pretty && yarn lint:md",
    "lint:md": "npx markdownlint ./content",
    "lint:pretty": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "lint:sort": "npx ./node_modules/@luddites-me/ts-tools/dist/lint/sortJson.js",
    "lint:ts": "eslint --cache --fix \"src/**/*.{ts,tsx}\"",
    "lint:type-check": "tsc --noEmit",
    "serve": "gatsby build && gatsby serve",
    "start": "gatsby develop"
  },
