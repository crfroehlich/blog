  "envDocumentation": {
    "ALGOLIA_ADMIN_API_KEY": {
      "defaultValue": "x",
      "description": "Algolia Search Admin API Key"
    },
    "ALGOLIA_APPLICATION_ID": {
      "defaultValue": "x",
      "description": "Algolia Search Application Id"
    },
    "ALGOLIA_SEARCH_ONLY_API_KEY": {
      "defaultValue": "x",
      "description": "Alogolia Search API Key"
    },
    "DOCS_CREATE_README_INDEX": {
      "defaultValue": true,
      "description": "If true, creates an index in the main README file for all markdown content"
    },
    "DOCS_CREATE_TOC": {
      "defaultValue": false,
      "description": "If true, create a Table of Contents for each markdown file"
    },
    "GH_ACCESS_TOKEN": {
      "defaultValue": "x",
      "description": "A read-write access token used for CI/CD"
    },
    "IGNORE_MARKDOWN_FILES": {
      "defaultValue": ".yarn,.vscode,.github,.tmp,temp,node_modules,.git,.cache,public",
      "description": "A comma-delimited list of markdown files to exclude from processing"
    },
    "IGNORE_PEER_DEPENDENCIES": {
      "defaultValue": "",
      "description": "A list of packages to exclude from peer dependencies."
    },
    "NODE_ENV": {
      "defaultValue": "dev",
      "description": "Environment. Options are: dev, test and prod."
    },
    "SYNC_PEER_DEPENDENCIES": {
      "defaultValue": false,
      "description": "If true, sets all project dependencies as peer dependencies"
    }
  },
  "license": "MIT",
  "main": "n/a",
  "name": "blog",
  "scripts": {
    "build": "gatsby build",
    "build:codegen": "graphql-codegen",
    "build:prep": "yarn lint && yarn build:index && yarn docs:standardize",
    "build:index": "npx cti create ./src -w -b",
    "clean": "gatsby clean && rimraf .cache .tmp public __generated__ dist src/**/__generated__/**",
    "debug:dev": "node --nolazy node_modules/.bin/gatsby develop --inspect-brk",
    "debug:prod": "node --nolazy node_modules/.bin/gatsby build --inspect-brk",
    "deploy": "yarn build && node build/publish.js",
    "docs:all": "yarn docs:standardize && yarn docs:api",
    "docs:api": "rimraf dist && tsc -p tsconfig.tools.json && node ./node_modules/@luddites-me/ts-tools/dist/docs/generateApi.js --path=./api-extractor.json",
    "docs:publish": "node ./node_modules/@luddites-me/ts-tools/dist/docs/generateApiDocs.js",
    "docs:standardize": "node ./node_modules/@luddites-me/ts-tools/dist/readme/standardizeReadme.js",
    "lint": "pretty-quick --staged && yarn lint:sort && yarn lint:ts && yarn lint:pretty",
    "lint:md": "npx markdownlint ./content",
    "lint:pretty": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "lint:sort": "npx ./node_modules/@luddites-me/ts-tools/dist/lint/sortJson.js",
    "lint:ts": "eslint --cache --fix \"src/**/*.{ts,tsx}\"",
    "lint:type-check": "tsc --noEmit",
    "serve": "gatsby build && gatsby serve",
    "start": "gatsby develop"
  },
  "scriptsDocumentation": {
    "build": {
      "description": "Produces the production build of the site",
      "dev": false
    },
    "build:codegen": {
      "description": "Generate schema from GraphQL interfaces",
      "dev": true
    },
    "build:prep": {
      "description": "Preps the codebase for building",
      "dev": true
    },
    "clean": {
      "description": "Purges all temporary and generated folders/content",
      "dev": true
    },
    "debug:dev": {
      "description": "Start the app in development mode with the debugger attached",
      "dev": true
    },
    "debug:prod": {
      "description": "Start the app in production mode with the debugger attached",
      "dev": true
    },
    "deploy": {
      "description": "Publish the app to Github Pages",
      "dev": true
    },
    "docs:standardize": {
      "description": "Creates or updates a new readme with a standard set of readme sections, including a toc, yarn script documentation, links to repo documentation files and \"The Unlicense\" license.",
      "dev": true
    },
    "lint": {
      "description": "Lints the codebase.",
      "dev": true
    },
    "lint:md": {
      "description": "Runs lint rules on the Markdown content",
      "dev": true
    },
    "lint:pretty": {
      "description": "Run prettier rules and automatically fix content",
      "dev": true
    },
    "lint:sort": {
      "description": "Sorts all tracked JSON files",
      "dev": true
    },
    "lint:ts": {
      "description": "Runs ESLint on the TypeScript files",
      "dev": true
    },
    "lint:type-check": {
      "description": "Runs the tsc compiler against the code",
      "dev": true
    },
    "serve": {
      "description": "Starts the app in production mode",
      "dev": true
    },
    "start": {
      "description": "Starts the app in development mode",
      "dev": true
    }
  },
  "version": "0.9.2"
}
